                                                value="default_oneboxes"/>
                                            <input type="search" aria-label="Search Input" name="q"/>
                                            <button type="submit" aria-label="Submit Search"><i
                                                class="fa fa-search"><!-- keep --></i></button>
                                        </form>
                                    </div>
                                    <nav class="dosa-banner-navigation" role="navigation"
