                                            </ul>
                                        </div><!--
                          --><div
                                            class="section nuhelp closed">
                                            <h2 tabindex="0"><span class="inner">NUHelp</span></h2>
                                            <ul>
                                                <system-region name="NUHELP_DESKTOP"/>
                                            </ul>
                                        </div>
                                    </nav>
                                    <div class="northwestern-title">Northwestern University</div>
                                </div>
                                <div class="inner-right"/>
